Algoritmo Ejercicio_22
	// Variables
    Definir paginas, precioPagina, costoTotal Como Real;
	
    //Cantidad de páginas
    Escribir "Ingrese la cantidad de páginas a imprimir: ";
    Leer paginas;
	
    // Precio por página
    Escribir "Ingrese el precio por página: ";
    Leer precioPagina;
	
    // Costo total
    costoTotal <- paginas * precioPagina;
	
    Escribir "El costo total de impresión es: ", costoTotal;
FinAlgoritmo
