Algoritmo Ganancia_Neta
	// Variables
    Definir ingresos, gastos, gananciaNeta Como Real;
	
    // Pedir datos al usuario
    Escribir "Ingrese el total de ingresos: ";
    Leer ingresos;
	
    Escribir "Ingrese el total de gastos: ";
    Leer gastos;
	
    // Calcular la ganancia neta
    gananciaNeta <- ingresos - gastos;
	
    // Mostrar resultado
    Escribir "La ganancia neta es: ", gananciaNeta;
FinAlgoritmo
